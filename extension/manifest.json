{
  "manifest_version": 2,
  "name":"DeepCite",
  "version": "0.1",
  "browser_action": {
    "default_popup": "popup.html"
  },
  "background": {
    "scripts": ["js/background.js"]
  },
  "permissions": [
    "contextMenus",
    "storage"
  ],
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["js/jquery-3.2.1.min.js","js/content.js"]
    }
  ]
}
