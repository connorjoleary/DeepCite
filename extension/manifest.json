{
  "manifest_version": 2,
  "name":"DeepCite",
  "version": "0.3",
  "browser_action": {
    "default_popup": "popup.html"
  },
  "icons": { "16": "images/icon16.png",
    "48": "images/icon48.png",
   "128": "images/icon128.png" 
  },

  "background": {
    "scripts": ["js/background.js"]
  },
  "permissions": [
    "storage"
  ],
  "content_security_policy": "script-src 'self' https://sdk.amazonaws.com/js/aws-sdk-2.713.0.min.js; object-src 'self'",
  "content_scripts": [
    {
      "matches": ["https://jzvkkf7p6d.execute-api.us-east-2.amazonaws.com/*", "http://localhost:8001/*"],
      "js": ["js/jquery-3.2.1.min.js"]
    }
  ]
}
